{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "svelte-fa/src/fa.svelte": ["node_modules/svelte-fa/src/index.d.ts"]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
